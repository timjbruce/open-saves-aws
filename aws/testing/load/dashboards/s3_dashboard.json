          [ ".", "PutRequests", ".", ".", ".", "." ],
          [ ".", "DeleteRequests", ".", ".", ".", "." ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${Region}",
        "title": "S3 Request Count",
        "period": 60,
        "stat": "Sum"
      }
    },
    {
      "type": "metric",
