{
  "widgets": [
    {
      "type": "metric",
      "x": 0,
      "y": 0,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "AWS/S3", "AllRequests", "BucketName", "${BucketName}", "FilterId", "EntireBucket" ],
