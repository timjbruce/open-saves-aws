{
  "widgets": [
    {
      "type": "metric",
      "x": 0,
      "y": 0,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "AWS/ElastiCache", "CPUUtilization", "CacheClusterId", "${CacheClusterId}" ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${Region}",
        "title": "Redis CPU Utilization",
        "period": 60,
        "stat": "Average",
        "yAxis": {
          "left": {
            "min": 0,
            "max": 100
          }
        }
      }
    },
    {
      "type": "metric",
      "x": 12,
      "y": 0,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "AWS/ElastiCache", "FreeableMemory", "CacheClusterId", "${CacheClusterId}" ],
          [ ".", "SwapUsage", ".", "." ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${Region}",
        "title": "Redis Memory Usage",
        "period": 60,
        "stat": "Average"
      }
    },
    {
      "type": "metric",
      "x": 0,
      "y": 6,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "AWS/ElastiCache", "CacheHits", "CacheClusterId", "${CacheClusterId}" ],
          [ ".", "CacheMisses", ".", "." ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${Region}",
        "title": "Redis Cache Hits/Misses",
        "period": 60,
        "stat": "Sum"
      }
    },
    {
      "type": "metric",
      "x": 12,
      "y": 6,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ { "expression": "m1/(m1+m2)*100", "label": "Cache Hit Rate (%)", "id": "e1", "region": "${Region}" } ],
          [ "AWS/ElastiCache", "CacheHits", "CacheClusterId", "${CacheClusterId}", { "id": "m1", "visible": false } ],
          [ ".", "CacheMisses", ".", ".", { "id": "m2", "visible": false } ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${Region}",
        "title": "Redis Cache Hit Rate",
        "period": 60,
        "stat": "Sum",
        "yAxis": {
          "left": {
            "min": 0,
            "max": 100
          }
        }
      }
    },
    {
      "type": "metric",
      "x": 0,
      "y": 12,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "AWS/ElastiCache", "NetworkBytesIn", "CacheClusterId", "${CacheClusterId}" ],
          [ ".", "NetworkBytesOut", ".", "." ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${Region}",
        "title": "Redis Network Traffic",
        "period": 60,
        "stat": "Sum"
      }
    },
    {
      "type": "metric",
      "x": 12,
      "y": 12,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "AWS/ElastiCache", "CurrConnections", "CacheClusterId", "${CacheClusterId}" ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${Region}",
        "title": "Redis Current Connections",
        "period": 60,
        "stat": "Average"
      }
    },
    {
      "type": "metric",
      "x": 0,
      "y": 18,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "AWS/ElastiCache", "NewConnections", "CacheClusterId", "${CacheClusterId}" ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${Region}",
        "title": "Redis New Connections",
        "period": 60,
        "stat": "Sum"
      }
    },
    {
      "type": "metric",
      "x": 12,
      "y": 18,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "AWS/ElastiCache", "Evictions", "CacheClusterId", "${CacheClusterId}" ],
          [ ".", "Reclaimed", ".", "." ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${Region}",
        "title": "Redis Evictions and Reclaimed",
        "period": 60,
        "stat": "Sum"
      }
    }
  ]
}
